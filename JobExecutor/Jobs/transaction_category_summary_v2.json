{
  "jobName": "TransactionCategorySummaryV2",
  "firstEffectiveDate": "2024-10-01",
  "modules": [
    {
      "type": "DataSourcing",
      "resultName": "transactions",
      "schema": "datalake",
      "table": "transactions",
      "columns": ["txn_type", "amount"]
    },
    {
      "type": "Transformation",
      "resultName": "txn_cat_summary",
      "sql": "SELECT txn_type, as_of, ROUND(SUM(amount), 2) AS total_amount, COUNT(*) AS transaction_count, ROUND(AVG(amount), 2) AS avg_amount FROM transactions GROUP BY txn_type, as_of ORDER BY as_of, txn_type"
    },
    {
      "type": "DataFrameWriter",
      "source": "txn_cat_summary",
      "targetTable": "transaction_category_summary",
      "writeMode": "Append",
      "targetSchema": "double_secret_curated"
    }
  ]
}
