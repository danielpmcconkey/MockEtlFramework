{
  "jobName": "MonthlyTransactionTrendV2",
  "firstEffectiveDate": "2024-10-01",
  "modules": [
    {
      "type": "DataSourcing",
      "resultName": "daily_volume",
      "schema": "curated",
      "table": "daily_transaction_volume",
      "columns": ["total_transactions", "total_amount", "avg_amount"]
    },
    {
      "type": "Transformation",
      "resultName": "monthly_trend",
      "sql": "SELECT as_of, total_transactions AS daily_transactions, total_amount AS daily_amount, avg_amount AS avg_transaction_amount FROM daily_volume ORDER BY as_of"
    },
    {
      "type": "DataFrameWriter",
      "source": "monthly_trend",
      "targetTable": "monthly_transaction_trend",
      "writeMode": "Append",
      "targetSchema": "double_secret_curated"
    }
  ]
}
